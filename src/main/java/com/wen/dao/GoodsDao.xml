<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wen.dao.GoodsDao">
<!-- 一对一关系:扩展类 ，association(支持延迟加载)-->	
	
	<insert id="addGoods" parameterType="goods">
	insert into goods(name,picture,price,url,catalog_name)values(#{name},#{picture},#{price},#{url},#{catalog_name})
	
	</insert>
	
</mapper>